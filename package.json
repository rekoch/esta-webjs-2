{
  "name": "webjs-starterkit",
  "description": "This is a starterkit based on angular-cli with enterprise extensions",
  "version": "4.1.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "ng lint --type-check",
    "test": "ng test --watch=false",
    "test-selenium": "ng test --progress=false --watch=false --browsers=SeleniumCH,SeleniumFF --code-coverage",
    "e2e": "ng e2e",
    "e2e-selenium": "ng e2e --progress=false --disableHostCheck=true --host=0.0.0.0 --port=$(echo $externalport) --public-host=$(my-local-ip):$(echo $externalport) --config=protractor.webgrid.conf.js",
    "prebuild-prod": "npm run test-selenium",
    "build-prod": "ng build --prod --progress=false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.2",
    "@angular/cli": "^1.1.1",
    "@angular/common": "^4.2.2",
    "@angular/compiler": "^4.2.2",
    "@angular/compiler-cli": "^4.2.2",
    "@angular/core": "^4.2.2",
    "@angular/forms": "^4.2.2",
    "@angular/http": "^4.2.2",
    "@angular/platform-browser": "^4.2.2",
    "@angular/platform-browser-dynamic": "^4.2.2",
    "@angular/router": "^4.2.2",
    "@ngx-translate/core": "^7.0.0",
    "@ngx-translate/http-loader": "^0.1.0",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "keycloak-js": "^3.1.0",
    "primeng": "^4.0.3",
    "rxjs": "^5.4.0",
    "typescript": "^2.3.4",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "^1.1.1",
    "@angular/compiler-cli": "^4.2.2",
    "@types/jasmine": "^2.5.51",
    "@types/node": "^7.0.29",
    "codelyzer": "^3.0.1",
    "jasmine-core": "^2.6.3",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-nightmare": "^0.4.8",
    "karma-webdriver-launcher": "^1.0.5",
    "my-local-ip": "^1.0.0",
    "nightmare": "^2.10.0",
    "protractor": "^5.1.2",
    "ts-node": "^3.0.6",
    "tslint": "^5.4.3",
    "typescript": "^2.3.4"
  },
  "keywords": [
    "esta-webjs-starterkit",
    "angularjs",
    "webpack"
  ],
  "author": {
    "name": "Reto Lehmann",
    "email": "reto.lehmann@sbb.ch"
  }
}
